{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/nested/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/nested",
        "main": "packages/nested/src/index.ts",
        "tsConfig": "packages/nested/tsconfig.base.json",
        "assets": ["packages/nested/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/nested/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/nested"],
      "options": {
        "jestConfig": "packages/nested/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
