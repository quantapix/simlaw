{
  "name": "@simlaw/simsrc",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "npx prisma format",
    "init": "npx prisma migrate dev --name init",
    "gen": "graphql-codegen --config ./gql.yml",
    "dev": "ts-node src/index.ts",
    "dev2": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema"
  },
  "dependencies": {
    "@koa/router": "",
    "@prisma/client": "",
    "koa": "",
    "koa-body": "",
    "express": "",
    "express-graphql": "",
    "graphql": "",
    "graphql-scalars": "",
    "nexus": "",
    "content-disposition": "",
    "cookies": "",
    "http-assert": "",
    "keygrip": "",
    "parseurl": "",
    "supertest": ""
  },
  "devDependencies": {
    "@types/express": "",
    "@types/koa": "",
    "@types/koa__router": "",
    "@types/node": "",
    "prisma": "",
    "ts-node": "",
    "ts-node-dev": "",
    "typescript": "",
    "@types/content-disposition": "",
    "@types/content-type": "",
    "@types/cookies": "",
    "@types/fresh": "",
    "@types/http-assert": "",
    "@types/http-errors": "",
    "@types/keygrip": "",
    "@types/methods": "",
    "@types/mime-types": "",
    "@types/on-finished": "",
    "@types/parseurl": "",
    "@types/statuses": "",
    "@types/supertest": "",
    "@types/type-is": "",
    "@types/vary": ""
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  }
}
